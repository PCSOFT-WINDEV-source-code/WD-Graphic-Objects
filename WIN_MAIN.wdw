     name : UPDButtonsState
     internal_properties : CAAAAAgAAAC3XIrAPU3dxPS3WxkDu16ghzngQkY3n//yPB69+saFvSexCXAjzcjwZ9qr/VJEnJMZ1hdqI56Te/uhwUjUNetJRbkbbmf2nzGQRwNuxHR+s/GYxdzobDQZWAVDZ0KDJAOh0LL4t5DXXHw3w7sTLQ9H0BZ6/Av9+FA/blrt/Ss4Ow==
     procedure_id : 1160397363367956073
     type_code : 14
     code : |1-
      // Summary: Update the state of buttons
      // Syntax:
      //UPDButtonsState ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Automatic procedure:
      // The procedure is run manually, during a call in the code
      // It will be repeated in loop, with 50 hundredths of a second between each call
      // A new timer is created for each call to the procedure
      // 
      
      procedure UPDButtonsState()
      
      if Empty then
      	
      	// Disables all the tools
      	DisableTool(BTN_SAVE, 
      	BTN_INS_IMAGE, 
      	BTN_INS_SHAPE, 
      	BTN_CANCEL, 
      	BTN_RESTORE, 
      	BTN_ALIGNMENT, 
      	BTN_STYLE, 
      	BTN_COPY, 
      	BTN_CUT, 
      	BTN_PASTE)		
      	
      else
      	
      	// Enables the tools
      	EnableTool(BTN_SAVE, 
      	BTN_INS_IMAGE, 
      	BTN_INS_SHAPE)
      	
      	// UNDO
      	if CIW_DOCUMENT.AAFCanUndo() then
      		EnableTool(BTN_CANCEL)
      	else
      		DisableTool(BTN_CANCEL)			
      	end
      	
      	// REDO
      	if CIW_DOCUMENT.AAFCanRedo() then
      		EnableTool(BTN_RESTORE)
      	else
      		DisableTool(BTN_RESTORE)			
      	end
      	
      	// PASTE
      	if ClipboardFormat(CF_CONTROLOBJECT) then
      		EnableTool(BTN_PASTE)
      	else
      		DisableTool(BTN_PASTE)
      	end
      	
      	// ALIGNMENT, COPY/CUT
      	if WinEdSelectOccurrence(CIW_DOCUMENT) > 0 then
      		
      		// If an object is selected
      		if WinEdSelectOccurrence(CIW_DOCUMENT) = 1 then
      			
      			// Retrieve the selected object
      			oSelection is Control <- WinEdSelect(CIW_DOCUMENT,1)
      			
      			// If it is a link
      			if oSelection <> null _and_ oSelection..Type = typWire then
      				
      				// No style, alignment or clipboard possible
      				DisableTool(BTN_ALIGNMENT, 
      				BTN_STYLE, 
      				BTN_COPY, 
      				BTN_CUT)
      				
      				return
      			end
      		end
      		
      		
      		EnableTool(BTN_ALIGNMENT, 
      		BTN_STYLE, 
      		BTN_COPY, 
      		BTN_CUT)
      		
      	else
      		DisableTool(BTN_ALIGNMENT, 
      		BTN_STYLE, 
      		BTN_COPY, 
      		BTN_CUT)
      		
      		
      	end
      end
      
      // Disables one or more controls (style rather than ..Grayed)
      INTERNAL PROCEDURE DisableTool(*)
      	for i = 1 _to_ MyParameters..NbReceived
      		if MyParameters[i]..State <> Inactive then
      			MyParameters[i]..State		= Inactive
      			MyParameters[i]..Opacity	= 50
      		end
      	end
      end
      
      // Enables one or more controls (style rather than ..Grayed)
      INTERNAL PROCEDURE EnableTool(*)
      	for i = 1 _to_ MyParameters..NbReceived
      		if MyParameters[i]..State <> Active then
      			MyParameters[i]..State		= Active
      			MyParameters[i]..Opacity	= 100
      		end
      	end
      end
     type : 458752
