    name : BTN_SAVE
    identifier : 0x201a8f4113e4ce69
    internal_properties : 
    properties :
     x : 109
     y : 4
     width : 52
     height : 48
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
         sFile is string
         
         // No current document
         if CIW_DOCUMENT.SourceWindow = "" then return
         
         // The opened document was saved in ..Note of the control
         // If ..Note is empty, it is a blank document
         if CIW_DOCUMENT.Note = "" then
         	
         	// Opens the file picker
         	sFile = fSelect("", "", "Save as...", "All the files (*.*)" + TAB + "*.*" + CR + "Graphic document (*.WDDOC)" + TAB + "*.WDDOC", "WDDOC", fselCreate + fselExist)
         	if sFile = "" then
         		return
         	end
         	
         else
         	
         	// File opened
         	sFile = CIW_DOCUMENT.Note	
         end
         
         HourGlass(True)
         
         // Full backup
         if WinEdSave(CIW_DOCUMENT, sFile, weModeFull) then
         	
         	// Loads the internal window
         	ChangeSourceWindow(CIW_DOCUMENT, IW_WORKSPACE, "", sFile)	
         	
         	// Title
         	MyWindow.Title			= ProjectInfo(piProjectName) + " - " + fExtractPath(sFile, fFileName + fExtension)
         	
         	// Stores the file name for next backup
         	CIW_DOCUMENT.Note		= sFile
         	CIW_DOCUMENT.Visible	= True
         	
         	// Inform that the backup was successful	
         	ToastDisplay("Document saved", toastShort, vaMiddle, haCenter)
         else
         	Error()
         end
         
         HourGlass(False)
